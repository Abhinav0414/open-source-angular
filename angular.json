{
  "version": 2,
  "projects": {
    "demos": {
      "projectType": "library",
      "root": "libs/demos",
      "sourceRoot": "libs/demos/src",
      "prefix": "demo",
      "targets": {
        "prebuild": {
          "executor": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/demos/tsconfig.lib.json",
            "project": "libs/demos/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/demos/tsconfig.lib.prod.json"
            }
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": ["yarn nx run demos:prebuild:production", "yarn"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/demos/src/**/*.ts",
              "libs/demos/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "forms": {
      "projectType": "library",
      "root": "libs/forms",
      "sourceRoot": "libs/forms/src",
      "prefix": "dyn",
      "targets": {
        "prebuild": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/libs/forms"],
          "options": {
            "tsConfig": "libs/forms/tsconfig.lib.json",
            "project": "libs/forms/ng-package.json",
            "updateBuildableProjectDepsInPackageJson": false
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/forms/tsconfig.lib.prod.json"
            }
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "yarn nx run forms:prebuild:production",
              "./node_modules/.bin/tsc -p libs/forms/tsconfig.schematics.json",
              "rsync -R libs/forms/lib/schematics/*/files/**/* dist/",
              "rsync -R libs/forms/lib/schematics/*/schema.json dist/"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/forms/**/*.ts", "libs/forms/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/forms"],
          "options": {
            "jestConfig": "libs/forms/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "preset": "angular",
            "tagPrefix": "@myndpm/dyn-forms@",
            "commitMessageFormat": "ci: release @myndpm/dyn-forms@${version}",
            "postTargets": ["forms:npm"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "tag": "${tag}",
            "notes": "${notes}"
          }
        },
        "npm": {
          "executor": "./dist/plugin:deploy-npm",
          "options": {
            "copy": true,
            "outputTarget": "prebuild",
            "target": "build"
          }
        }
      },
      "tags": ["shared"]
    },
    "plugin": {
      "root": "libs/plugin",
      "sourceRoot": "libs/plugin/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/plugin/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/plugin"],
          "options": {
            "jestConfig": "libs/plugin/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/plugin",
            "tsConfig": "libs/plugin/tsconfig.lib.json",
            "packageJson": "libs/plugin/package.json",
            "main": "libs/plugin/src/index.ts",
            "assets": [
              "libs/plugin/*.md",
              {
                "input": "./libs/plugin/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./libs/plugin/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./libs/plugin",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./libs/plugin",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "preset": "angular",
            "tagPrefix": "@myndpm/nx@",
            "commitMessageFormat": "ci: release @myndpm/nx@${version}",
            "postTargets": ["plugin:npm"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "tag": "${tag}",
            "notes": "${notes}"
          }
        },
        "npm": {
          "executor": "./dist/plugin:deploy-npm",
          "options": {
            "copy": true,
            "target": "build"
          }
        }
      },
      "tags": ["plugin"]
    },
    "pug2html": {
      "root": "libs/tools/pug2html",
      "sourceRoot": "libs/tools/pug2html/src",
      "projectType": "library",
      "targets": {
        "prebuild": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/pug2html",
            "tsConfig": "libs/tools/pug2html/tsconfig.lib.json",
            "packageJson": "libs/tools/pug2html/package.json",
            "main": "libs/tools/pug2html/src/index.ts",
            "assets": ["libs/tools/pug2html/*.md"],
            "deleteOutputPath": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "yarn nx run pug2html:prebuild",
              "chmod +x dist/libs/pug2html/src/index.js"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/tools/pug2html/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tools/pug2html"],
          "options": {
            "jestConfig": "libs/tools/pug2html/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "preset": "angular",
            "tagPrefix": "@myndpm/pug2html@",
            "commitMessageFormat": "ci: release @myndpm/pug2html@${version}",
            "postTargets": ["pug2html:npm"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "tag": "${tag}",
            "notes": "${notes}"
          }
        },
        "npm": {
          "executor": "./dist/plugin:deploy-npm",
          "options": {
            "copy": true,
            "outputTarget": "prebuild",
            "target": "build"
          }
        }
      },
      "tags": ["tools"]
    },
    "stylus2scss": {
      "root": "libs/tools/stylus2scss",
      "sourceRoot": "libs/tools/stylus2scss/src",
      "projectType": "library",
      "targets": {
        "prebuild": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/stylus2scss",
            "tsConfig": "libs/tools/stylus2scss/tsconfig.lib.json",
            "packageJson": "libs/tools/stylus2scss/package.json",
            "main": "libs/tools/stylus2scss/src/index.ts",
            "assets": ["libs/tools/stylus2scss/*.md"],
            "deleteOutputPath": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "yarn nx run stylus2scss:prebuild",
              "chmod +x dist/libs/stylus2scss/src/index.js"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/tools/stylus2scss/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tools/stylus2scss"],
          "options": {
            "jestConfig": "libs/tools/stylus2scss/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "preset": "angular",
            "tagPrefix": "@myndpm/stylus2scss@",
            "commitMessageFormat": "ci: release @myndpm/stylus2scss@${version}",
            "postTargets": ["stylus2scss:npm"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "tag": "${tag}",
            "notes": "${notes}"
          }
        },
        "npm": {
          "executor": "./dist/plugin:deploy-npm",
          "options": {
            "copy": true,
            "outputTarget": "prebuild",
            "target": "build"
          }
        }
      },
      "tags": ["tools"]
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "prebuild": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils",
            "tsConfig": "libs/utils/tsconfig.lib.json",
            "packageJson": "libs/utils/package.json",
            "main": "libs/utils/src/index.ts",
            "assets": ["libs/utils/*.md"],
            "deleteOutputPath": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "commands": [
              "yarn nx run utils:prebuild",
              "rimraf dist/libs/utils/node_modules/rxjs"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "preset": "angular",
            "tagPrefix": "@myndpm/utils@",
            "commitMessageFormat": "ci: release @myndpm/utils@${version}",
            "postTargets": ["utils:npm"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "tag": "${tag}",
            "notes": "${notes}"
          }
        },
        "npm": {
          "executor": "./dist/plugin:deploy-npm",
          "options": {
            "copy": true,
            "outputTarget": "prebuild",
            "target": "build"
          }
        }
      },
      "tags": ["shared"]
    },
    "website": {
      "projectType": "application",
      "root": "apps/website",
      "sourceRoot": "apps/website/src",
      "prefix": "app",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/website/browser",
            "index": "apps/website/src/index.html",
            "main": "apps/website/src/main.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "assets": [
              "apps/website/src/favicon.jpg",
              "apps/website/src/assets",
              {
                "glob": "**/*",
                "input": "docs",
                "output": "static"
              },
              {
                "glob": "**/*",
                "input": "libs/demos/dyn-forms",
                "output": "static/dyn-forms"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "apps/website/src/prism-mynd.css",
              "apps/website/src/material.scss",
              "apps/website/src/styles.scss"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-scss.min.js",
              "node_modules/prismjs/components/prism-typescript.js",
              "node_modules/prismjs/components/prism-bash.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "tsConfig": "apps/website/tsconfig.app.prod.json",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/website/src/**/*.ts",
              "apps/website/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/website"],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "executor": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/website/server",
            "main": "apps/website/server.ts",
            "tsConfig": "apps/website/tsconfig.server.json",
            "sourceMap": true,
            "optimization": false
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve-ssr": {
          "executor": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "website:build",
            "serverTarget": "website:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "website:build:production",
              "serverTarget": "website:server:production"
            }
          }
        },
        "prerender": {
          "executor": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "website:build:production",
            "serverTarget": "website:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        }
      },
      "implicitDependencies": ["demos"],
      "tags": []
    }
  }
}
