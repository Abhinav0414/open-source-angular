{
  "name": "@myndpm/open-source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "libs/**/*"
  ],
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0"
  },
  "scripts": {
    "start": "ng serve",
    "clean:forms": "rimraf dist/libs/forms",
    "build:forms": "run-s 'clean:forms' 'build forms -- --configuration production' 'nx postbuild forms'",
    "build:libs": "run-s 'build forms' 'build demos --configuration production'",
    "build:tools": "run-s 'build:pug2html'",
    "build:pug2html": "run-s 'build pug2html' 'nx postbuild pug2html'",
    "build:prod": "run-s 'build forms' 'nx postbuild forms' 'build demos --configuration production' 'yarn' 'build --configuration production'",
    "build": "ng build",
    "test": "ng test",
    "test:tdd": "ng test --bail 5 --cache --notify --notify-mode change --only-changed --watch",
    "lint": "nx workspace-lint && ng lint",
    "npm:publish": "npm publish --access=public",
    "npm:dyn-forms": "run-s 'build:forms' 'tools:postbuild libs/forms' 'npm:publish dist/libs/forms' 'npm:publish --registry=https://npm.pkg.github.com dist/libs/forms' 'tools:tag libs/forms'",
    "npm:pug2html": "run-s 'build:pug2html' 'npm:publish dist/libs/tools/pug2html' 'npm:publish --registry=https://npm.pkg.github.com dist/libs/tools/pug2html' 'tools:tag libs/tools/pug2html'",
    "tools:postbuild": "ts-node tools/scripts/post-build",
    "tools:tag": "ts-node tools/scripts/git-tag",
    "ssr:dev": "ng run website:serve-ssr",
    "ssr:serve": "node dist/website/server/main.js",
    "ssr:build": "ng build --configuration production && ng run website:server:production",
    "ssr:prerender": "ng run website:prerender",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "yarn": "yarn",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main"
  },
  "dependencies": {
    "@angular/animations": "12.2.13",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/flex-layout": "~12.0.0-beta.35",
    "@angular/forms": "12.2.13",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/platform-server": "12.2.13",
    "@angular/router": "12.2.13",
    "@nrwl/angular": "13.1.4",
    "bootstrap": "~5.1.3",
    "express": "^4.17.1",
    "fast-deep-equal": "~3.1.3",
    "is-callable": "~1.2.3",
    "marked": "~4.0.3",
    "prismjs": "~1.25.0",
    "rxjs": "6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.13",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@nguniversal/builders": "^12.1.3",
    "@nguniversal/express-engine": "~12.1.3",
    "@nrwl/cli": "13.1.4",
    "@nrwl/eslint-plugin-nx": "13.1.4",
    "@nrwl/jest": "13.1.4",
    "@nrwl/linter": "13.1.4",
    "@nrwl/tao": "13.1.4",
    "@nrwl/workspace": "13.1.4",
    "@nrwl/node": "13.1.4",
    "@types/express": "^4.17.11",
    "@types/is-callable": "~1.1.0",
    "@types/jest": "27.0.2",
    "@types/marked": "^4.0.0",
    "@types/node": "14.14.33",
    "@types/shelljs": "~0.8.8",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "angular-cli-ghpages": "~1.0.0-rc.2",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "ng-mocks": "~12.5.0",
    "ng-packagr": "12.2.5",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.4.1",
    "shelljs": "~0.8.4",
    "ts-jest": "27.0.5",
    "ts-node": "~10.4.0",
    "typescript": "4.3.5"
  }
}
